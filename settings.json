{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "env": {},
  "permissions": {
    "allow": [],
    "deny": []
  },
  "hooks": {
    "PreToolUse": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "uv run ~/.claude/hooks/pre_tool_use.py"
          }
        ]
      }
    ],
    "SessionStart": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "uv run ~/.claude/hooks/session_start.py"
          }
        ]
      }
    ],
    "Stop": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "uv run ~/.claude/hooks/stop.py --chat"
          }
        ]
      }
    ],
    "SubagentStop": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "uv run ~/.claude/hooks/subagent_stop.py"
          }
        ]
      }
    ],
    "UserPromptSubmit": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "uv run ~/.claude/hooks/user_prompt_submit.py --log-only"
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "bunx -y ccstatusline@latest",
    "padding": 0
  },
  "enabledPlugins": {
    "domain-memory-agent@claude-code-plugins": true,
    "workflow-orchestrator@claude-code-plugins": true,
    "overnight-dev@claude-code-plugins": true,
    "superpowers@superpowers-marketplace": true,
    "superpowers-chrome@superpowers-marketplace": false,
    "security-guidance@claude-code-plugins": false,
    "agent-sdk-dev@claude-code-plugins": false,
    "pr-review-toolkit@claude-code-plugins": false,
    "feature-dev@claude-code-plugins": false,
    "elements-of-style@superpowers-marketplace": false,
    "compounding-engineering@every-marketplace": false,
    "document-skills@anthropic-agent-skills": true,
    "llm-application-dev@claude-code-workflows": true,
    "data-engineering@claude-code-workflows": true,
    "data-validation-suite@claude-code-workflows": true,
    "context-management@claude-code-workflows": true,
    "code-refactoring@claude-code-workflows": true
  },
  "alwaysThinkingEnabled": true,
  "feedbackSurveyState": {
    "lastShownTime": 1754081769578
  },
  "model": "sonnet[1m]"
}
